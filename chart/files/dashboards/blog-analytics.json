{
  "uid": "victorbona-blog-analytics",
  "title": "victorbona.blog Analytics",
  "description": "Page views, traffic sources, device analytics, and Web Vitals",
  "tags": ["victorbona-blog", "analytics", "rum"],
  "timezone": "browser",
  "editable": false,
  "graphTooltip": 1,
  "schemaVersion": 42,
  "refresh": "30s",
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
  },
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus",
        "current": {},
        "hide": 0,
        "label": "Prometheus"
      },
      {
        "name": "loki",
        "type": "datasource",
        "query": "loki",
        "current": {},
        "hide": 0,
        "label": "Loki"
      }
    ]
  },
  "annotations": {
    "list": []
  },
  "panels": [
    {
      "type": "row",
      "title": "Summary Stats",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "panels": []
    },
    {
      "type": "stat",
      "title": "Total Page Views",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 1 },
      "id": 2,
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(increase(blog_page_views_total{is_bot=\"false\"}[$__range]))",
          "legendFormat": "Page Views"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        }
      }
    },
    {
      "type": "stat",
      "title": "Unique Sessions",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 1 },
      "id": 3,
      "datasource": {
        "type": "loki",
        "uid": "${loki}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "count(sum by (session_id) (count_over_time({job=\"faro\"} |= `app_name=victorbona-blog` | logfmt | session_id != `` [$__range])))",
          "legendFormat": "Sessions"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "textMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        }
      }
    },
    {
      "type": "stat",
      "title": "Top Page",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 1 },
      "id": 4,
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(1, sum by (path) (increase(blog_page_views_total{is_bot=\"false\"}[$__range])))",
          "legendFormat": "{{path}}",
          "instant": true,
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "purple", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "textMode": "name",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": "/^path$/"
        }
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true, "Value": false }
          }
        }
      ]
    },
    {
      "type": "stat",
      "title": "Avg Load Time (LCP)",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 1 },
      "id": 5,
      "datasource": {
        "type": "loki",
        "uid": "${loki}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "{job=\"faro\"} |= `app_name=victorbona-blog` |= `kind=measurement` |= `lcp=` | logfmt | unwrap lcp [$__range] | avg_over_time()",
          "legendFormat": "LCP"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 2500 },
              { "color": "red", "value": 4000 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "textMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        }
      }
    },
    {
      "type": "row",
      "title": "Traffic Trends - How much traffic?",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "id": 6,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Page Views Over Time",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 6 },
      "id": 7,
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(blog_page_views_total{is_bot=\"false\"}[$__rate_interval])) * 60",
          "legendFormat": "Views/min"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 25,
            "gradientMode": "opacity",
            "spanNulls": false,
            "pointSize": 5,
            "showPoints": "auto",
            "lineInterpolation": "smooth",
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "scaleDistribution": {
              "type": "linear"
            }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "single", "sort": "none" },
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true }
      }
    },
    {
      "type": "row",
      "title": "Top Pages",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "id": 8,
      "panels": []
    },
    {
      "type": "table",
      "title": "Top Pages Ranking",
      "gridPos": { "h": 7, "w": 24, "x": 0, "y": 15 },
      "id": 9,
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(10, sum by (path) (increase(blog_page_views_total{is_bot=\"false\"}[$__range])))",
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true },
            "renameByName": { "path": "Page", "Value": "Views" },
            "indexByName": { "Page": 0, "Views": 1 }
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "filterable": true,
            "cellOptions": {
              "type": "auto"
            }
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Views" },
            "properties": [
              { "id": "unit", "value": "short" },
              { "id": "decimals", "value": 0 }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "Views", "desc": true }],
        "cellHeight": "sm",
        "footer": {
          "show": false,
          "reducer": ["sum"]
        }
      }
    },
    {
      "type": "row",
      "title": "Traffic Sources - Where from?",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 22 },
      "id": 10,
      "panels": []
    },
    {
      "type": "piechart",
      "title": "Traffic Sources",
      "gridPos": { "h": 7, "w": 8, "x": 0, "y": 23 },
      "id": 11,
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (source) (increase(blog_page_views_total{is_bot=\"false\"}[$__range]))",
          "legendFormat": "{{source}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "pieType": "pie",
        "tooltip": { "mode": "single", "sort": "none" },
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": ["value", "percent"],
          "showLegend": true
        },
        "displayLabels": ["name", "percent"],
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        }
      }
    },
    {
      "type": "piechart",
      "title": "UTM Sources",
      "gridPos": { "h": 7, "w": 8, "x": 8, "y": 23 },
      "id": 12,
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (utm_source) (increase(blog_page_views_total{is_bot=\"false\", utm_source!=\"\"}[$__range]))",
          "legendFormat": "{{utm_source}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "pieType": "pie",
        "tooltip": { "mode": "single", "sort": "none" },
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": ["value", "percent"],
          "showLegend": true
        },
        "displayLabels": ["name", "percent"],
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        }
      }
    },
    {
      "type": "piechart",
      "title": "UTM Mediums",
      "gridPos": { "h": 7, "w": 8, "x": 16, "y": 23 },
      "id": 13,
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (utm_medium) (increase(blog_page_views_total{is_bot=\"false\", utm_medium!=\"\"}[$__range]))",
          "legendFormat": "{{utm_medium}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "pieType": "pie",
        "tooltip": { "mode": "single", "sort": "none" },
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": ["value", "percent"],
          "showLegend": true
        },
        "displayLabels": ["name", "percent"],
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        }
      }
    },
    {
      "type": "row",
      "title": "Devices - What devices?",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 30 },
      "id": 14,
      "panels": []
    },
    {
      "type": "piechart",
      "title": "Browsers",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 31 },
      "id": 15,
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (browser) (increase(blog_page_views_total{is_bot=\"false\"}[$__range]))",
          "legendFormat": "{{browser}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "pieType": "pie",
        "tooltip": { "mode": "single", "sort": "none" },
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": ["value", "percent"],
          "showLegend": true
        },
        "displayLabels": ["name", "percent"],
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        }
      }
    },
    {
      "type": "piechart",
      "title": "Devices",
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 31 },
      "id": 16,
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (device) (increase(blog_page_views_total{is_bot=\"false\"}[$__range]))",
          "legendFormat": "{{device}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "pieType": "pie",
        "tooltip": { "mode": "single", "sort": "none" },
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": ["value", "percent"],
          "showLegend": true
        },
        "displayLabels": ["name", "percent"],
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        }
      }
    },
    {
      "type": "row",
      "title": "Performance - How fast?",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 38 },
      "id": 17,
      "panels": []
    },
    {
      "type": "stat",
      "title": "Web Vitals Health",
      "description": "Overall Web Vitals assessment based on Core Web Vitals thresholds",
      "gridPos": { "h": 5, "w": 8, "x": 0, "y": 39 },
      "id": 18,
      "datasource": {
        "type": "loki",
        "uid": "${loki}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "{job=\"faro\"} |= `app_name=victorbona-blog` |= `kind=measurement` |= `lcp=` | logfmt | unwrap lcp [$__range] | quantile_over_time(0.75)",
          "legendFormat": "Health"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 2500 },
              { "color": "red", "value": 4000 }
            ]
          },
          "mappings": [
            {
              "type": "range",
              "options": {
                "from": 0,
                "to": 2500,
                "result": { "text": "Good", "color": "green" }
              }
            },
            {
              "type": "range",
              "options": {
                "from": 2500,
                "to": 4000,
                "result": { "text": "Needs Improvement", "color": "yellow" }
              }
            },
            {
              "type": "range",
              "options": {
                "from": 4000,
                "to": null,
                "result": { "text": "Poor", "color": "red" }
              }
            }
          ]
        },
        "overrides": []
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "textMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        }
      }
    },
    {
      "type": "stat",
      "title": "LCP (p75)",
      "description": "Largest Contentful Paint - measures loading performance. Good: <2.5s",
      "gridPos": { "h": 5, "w": 4, "x": 8, "y": 39 },
      "id": 19,
      "datasource": {
        "type": "loki",
        "uid": "${loki}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "{job=\"faro\"} |= `app_name=victorbona-blog` |= `kind=measurement` |= `lcp=` | logfmt | unwrap lcp [$__range] | quantile_over_time(0.75)",
          "legendFormat": "LCP"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 2500 },
              { "color": "red", "value": 4000 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "textMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        }
      }
    },
    {
      "type": "stat",
      "title": "INP (p75)",
      "description": "Interaction to Next Paint - measures responsiveness. Good: <200ms",
      "gridPos": { "h": 5, "w": 4, "x": 12, "y": 39 },
      "id": 20,
      "datasource": {
        "type": "loki",
        "uid": "${loki}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "{job=\"faro\"} |= `app_name=victorbona-blog` |= `kind=measurement` |= ` inp=` | logfmt | unwrap inp [$__range] | quantile_over_time(0.75)",
          "legendFormat": "INP"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 200 },
              { "color": "red", "value": 500 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "textMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        }
      }
    },
    {
      "type": "stat",
      "title": "CLS (p75)",
      "description": "Cumulative Layout Shift - measures visual stability. Good: <0.1",
      "gridPos": { "h": 5, "w": 4, "x": 16, "y": 39 },
      "id": 21,
      "datasource": {
        "type": "loki",
        "uid": "${loki}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "{job=\"faro\"} |= `app_name=victorbona-blog` |= `kind=measurement` |= ` cls=` | logfmt | unwrap cls [$__range] | quantile_over_time(0.75)",
          "legendFormat": "CLS"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 3,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.1 },
              { "color": "red", "value": 0.25 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "textMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        }
      }
    },
    {
      "type": "stat",
      "title": "JS Errors",
      "description": "JavaScript errors captured by Faro",
      "gridPos": { "h": 5, "w": 4, "x": 20, "y": 39 },
      "id": 22,
      "datasource": {
        "type": "loki",
        "uid": "${loki}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(count_over_time({job=\"faro\"} |= `app_name=victorbona-blog` |= `kind=exception` [$__range]))",
          "legendFormat": "Errors"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 10 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["sum"],
          "fields": ""
        }
      }
    }
  ]
}
